{
  "name": "react-konva-editor",
  "version": "0.2.0-r16",
  "author": "mytac",
  "description": "An image editor which is built by konva and react.",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cli": "node ./scripts/index.mjs",
    "build": "father build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint --fix --ext .js,.ts,jsx,tsx src",
    "dev:release": "./publish.sh -dev",
    "prod:release": "./publish.sh -prod",
    "npm-server": "verdaccio",
    "unrelease": "./unpublish.sh -dev",
    "prod:unrelease": "./unpublish.sh -prod",
    "type": " tsc --emitDeclarationOnly -outDir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mytac/react-konva-editor.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "keywords": [
    "editor",
    "react",
    "konva",
    "react-component"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mytac/react-konva-editor/issues"
  },
  "homepage": "https://github.com/mytac/react-konva-editor#readme",
  "dependencies": {
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "konva": "^8.2.3",
    "lodash": "^4.17.21",
    "react-konva": "16.8.0",
    "tinykeys": "^1.4.0",
    "typescript": "^4.9.5",
    "use-image": "1.0.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "@types/node": "^12.0.0",
    "@types/react": "^16.14.0",
    "eslint": "^7.11.0",
    "father": "4.1.8",
    "prettier": "^2.2.1",
    "react": "^16.14.0"

  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  }
}
