{
  "name": "react-konva-editor",
  "version": "0.1.1",
  "author": "mytac",
  "description": "An image editor which is built by konva and react.",
  "main": "./index.ts",
  "types": "./index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cli": "node ./scripts/index.mjs",
    "build1": "tsc -outDir ./dist  &&  tsc -module commonjs -outDir ./lib",
    "build2": "tsup ./index.ts",
    "build": "webpack",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint --fix --ext .js,.ts,jsx,tsx src",
    "dev:release": "./publish.sh -dev",
    "prod:release": "./publish.sh -prod",
    "npm-server": "verdaccio",
    "unrelease":"./unpublish.sh -dev",
    "type":" tsc --emitDeclarationOnly -outDir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mytac/react-konva-editor.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "keywords": [
    "editor",
    "react",
    "konva"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mytac/react-konva-editor/issues"
  },
  "homepage": "https://github.com/mytac/react-konva-editor#readme",
  "dependencies": {
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "konva": "^8.2.3",
    "lodash": "^4.17.21",
    "react": "16.14.0",
    "react-konva": "^17.0.2-5",
    "tinykeys": "^1.4.0",
    "typescript": "^4.9.5",
    "use-image": "^1.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/lodash": "^4.14.177",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "babel-loader": "^9.1.3",
    "eslint": "^7.11.0",
    "filehound": "^1.17.6",
    "prettier": "^2.2.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
